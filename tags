!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
age	hello.cpp	/^	int age;$/;"	m	struct:user	file:	access:public
fileinfo	include/hello.h	/^struct fileinfo$/;"	s
fileinfo::name	include/hello.h	/^	char name[256];$/;"	m	struct:fileinfo	access:public
fileinfo::size	include/hello.h	/^	int size;$/;"	m	struct:fileinfo	access:public
format	include/tags	/^!_TAG_FILE_FORMAT	2	\/extended format; --format=1 will not append ;" to lines\/$/;"	v
main	hello.cpp	/^int main()$/;"	f	signature:()
name	hello.cpp	/^	char name[64];$/;"	m	struct:user	file:	access:public
name	include/hello.h	/^	char name[256];$/;"	m	struct:fileinfo	access:public
size	include/hello.h	/^	int size;$/;"	m	struct:fileinfo	access:public
user	hello.cpp	/^struct user$/;"	s	file:
user::age	hello.cpp	/^	int age;$/;"	m	struct:user	file:	access:public
user::name	hello.cpp	/^	char name[64];$/;"	m	struct:user	file:	access:public
